<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="tagLibrary_elementDesc.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<div xmlns="http://www.tei-c.org/ns/1.0" xml:id="eventDesccription" type="elementDocumentation">
   <head>
      <gi>eventDescription</gi>
   </head>
   <div type="fullName">
      <p xml:lang="eng">Maintenance Event Description</p>
   </div>
   <div type="summary">
      <p xml:lang="eng">The description of a maintenance event in the life of the EAC-CPF instance.</p>
   </div>
   <div type="description" xml:lang="eng">
      <p>An element within the <gi>maintenanceEvent</gi> element for describing the maintenance event. The element allows a full description of the maintenance event to be given alongside the basic definition of the event in the
            <gi>eventType</gi> element.</p>
   </div>
   <div type="mayContain">
      <p>[text]</p>
   </div>
   <div type="mayOccurWithin">
      <p>maintenanceEvent</p>
   </div>
   <div type="attributes">
      <p xml:lang="eng">
         <list type="gloss">
            <label>xml:id</label>
            <item>Optional</item>
            <label>xml:lang</label>
            <item>Optional</item>
         </list>
      </p>
   </div>
   <div type="reference">
      <p>ISAAR (CPF) 5.4.9</p>
   </div>
   <div type="occurrence">
      <p>0...1</p>
   </div>
   <div type="examples">
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <eac-cpf:maintenanceEvent xmlns:eac-cpf="urn:isbn:1-931666-33-4">
            <eac-cpf:eventType>created</eac-cpf:eventType>
            <eac-cpf:eventDateTime standardDateTime="2009-11-30"/>
            <eac-cpf:agentType>human</eac-cpf:agentType>
            <eac-cpf:agent>Bill Stockting</eac-cpf:agent>
            <eac-cpf:eventDescription>Created from original in ISAAR (CPF), 2nd edition: example 10</eac-cpf:eventDescription>
         </eac-cpf:maintenanceEvent>
      </egXML>
   </div>
</div>
