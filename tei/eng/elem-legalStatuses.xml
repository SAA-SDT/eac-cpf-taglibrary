            <eac-cpf:legalStatus>
               <eac-cpf:term>Private limited liability company</eac-cpf:term>
               <eac-cpf:dateRange>
                  <eac-cpf:fromDate standardDate="1941">1941</eac-cpf:fromDate>
                  <eac-cpf:toDate standardDate="1948">1948</eac-cpf:toDate>
               </eac-cpf:dateRange>
            </eac-cpf:legalStatus>
            <eac-cpf:legalStatus>
               <eac-cpf:term>Public limited liability company</eac-cpf:term>
               <eac-cpf:dateRange>
                  <eac-cpf:fromDate standardDate="1948">1948</eac-cpf:fromDate>
                  <eac-cpf:toDate standardDate="2006">2006</eac-cpf:toDate>
               </eac-cpf:dateRange>
            </eac-cpf:legalStatus>
            <eac-cpf:legalStatus>
               <eac-cpf:term>Private limited liability company</eac-cpf:term>
               <eac-cpf:dateRange>
                  <eac-cpf:fromDate standardDate="2006">2006</eac-cpf:fromDate>
                  <eac-cpf:toDate standardDate="2008">2008</eac-cpf:toDate>
