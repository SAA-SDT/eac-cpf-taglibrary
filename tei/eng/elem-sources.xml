         <list type="gloss">
            <label>xml:base</label>
            <item>Optional</item>
            <label>xml:id</label>
            <item>Optional</item>
            <label>xml:lang</label>
            <item>Optional</item>
         </list>
      </p>
      
   </div>
   <div type="reference">
      <p>ISAAR (CPF) 5.4.8</p>
   </div>
   <div type="occurrence">
      <div type="mandatory">
         <p>Optional</p>
      </div>
      <div type="repeatable">
